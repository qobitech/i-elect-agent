  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --noEmit --pretty",
    "lint:eslint-check": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:eslint-fix": "eslint . --fix --report-unused-disable-directives --max-warnings 0",
